    <description>Profile Menu</description>
    <isExposed>true</isExposed>
    <masterLabel>Profile Menu</masterLabel>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property type="String" name="navigationLinkSetDevName" label="Navigation Set Dev. Name"/>
