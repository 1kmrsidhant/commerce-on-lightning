            <property type="String" name="guestUserIconName" label="Guest User Icon" default="utility:user"/>
            <property type="Boolean" name="showFirstName" label="Show User First Name" default="true"/>
            <property type="Boolean" name="showLastName" label="Show User Last Name" default="true"/>
